{
    "name": "krause-global/deal-desk-dashboard",
    "description": "Deal Desk Dashboard for Krause Global Resources & Trade",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-fileinfo": "*",
        "ext-zip": "*",
        "smalot/pdfparser": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Controllers\\": "app/Controllers/",
            "App\\Models\\": "app/Models/",
            "App\\Services\\": "app/Services/",
            "App\\Core\\": "app/Core/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
